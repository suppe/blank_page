
{
    "manifest_version": 3,
    "name": "blank_page",
    "description": "Just blank page with random text",
    "version": "0.1",
    "incognito": "split",
    "chrome_url_overrides": {
    "newtab": "snippets.html"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab", "storage"
    ],
    "host_permissions": [
        "https://ajax.googleapis.com/"
    ],
    "action": {
        "default_icon": "icons/icon16.png",
        "default_popup": "options.html"
    },
    "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" }
}