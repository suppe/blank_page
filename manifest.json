
{
    "manifest_version": 2,
    "name": "White Page",
    "description": "White Page with inspiring quotes on new page.",
    "version": "0.1",
    "incognito": "split",
    "chrome_url_overrides": {
    "newtab": "quotes.html"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab", "https://ajax.googleapis.com/", "storage"
    ],
    "browser_action": {
        "default_icon": "icons/icon16.png",
        "default_popup": "options.html"
    },
    "icons": { "16": "icons/icon16.png",
           "48": "icons/icon48.png",
          "128": "icons/icon128.png" }
}
